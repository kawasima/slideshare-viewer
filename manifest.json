{
  "background": {
    "scripts": ["javascript/jquery-2.1.1.min.js", "javascript/background.js"],
    "persistent": true
  },
  "manifest_version": 2,
  "name": "Slideshare Viewer",
  "version": "0.4.0",
  "description": "",
  "permissions": [
    "webRequest",
    "storage",
    "http://sssslide.com/*",
    "*://www.slideshare.net/*",
    "*://speakerdeck.com/*"
  ],
  "web_accessible_resources": [
    "slide.html"
  ]
}
