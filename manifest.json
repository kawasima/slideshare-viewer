{
  "manifest_version": 2,
  "name": "Slideshare Viewer",
  "version": "0.1.0",
  "description": "",
  "content_scripts": [
    {
      "matches": ["http://www.slideshare.net/*"],
      "js": ["jquery-2.1.1.min.js", "slideshare.js"]
    }
  ],
  "permissions": [
    "storage",
    "http://sssslide.com/*"
  ],
  "web_accessible_resources": [
    "slide.html"
  ]
}
